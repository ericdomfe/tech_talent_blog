<!DOCTYPE html>
<html>
<head>
  <title>Workspace</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
 
  <%= csrf_meta_tags %>
</head>
<body>

<div class="container">
  <div class='pull-right'>
    <%= link_to 'Sign Out', destroy_user_session_path, methods: :delete, class 'btnbtn.danger' %>
    <% end %>
  </div>
  <div class="pull-right">
    <% if user_signed_in? %>
     <%= link_to "Edit Profile", edit_user_registration_path, class: "btn btn-primary" %> <%= link_to "Sign Out", destroy_user_session_path, method: :delete, class: "btn btn-danger" %>
    <% end %>
  </div>
</div>
<%= yield %>
</div>
</body>
</html>
